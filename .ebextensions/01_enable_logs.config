files:
  "/etc/httpd/conf.d/wsgi.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      WSGIScriptAlias / /var/app/current/mlproject/wsgi.py
      WSGIDaemonProcess wsgi processes=1 threads=15 display-name=%{GROUP}
      WSGIProcessGroup wsgi

commands:
  enable_debug_log:
    command: "touch /tmp/debug.log && chmod 666 /tmp/debug.log"
